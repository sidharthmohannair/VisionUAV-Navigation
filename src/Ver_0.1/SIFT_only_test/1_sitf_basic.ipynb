    "import numpy as np\n",
    "import cv2 as cv\n",
    "\n",
    "img = cv.imread('home.jpg')\n",
    "gray= cv.cvtColor(img,cv.COLOR_BGR2GRAY)\n",
    "\n",
    "sift = cv.SIFT_create()\n",
    "kp = sift.detect(gray,None)\n",
    "\n",
    "img=cv.drawKeypoints(gray,kp,img)\n",
    "\n",
    "cv.imwrite('sift_keypoints.jpg',img)"
   ]
  }
